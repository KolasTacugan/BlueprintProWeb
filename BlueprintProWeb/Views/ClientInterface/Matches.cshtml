@model IEnumerable<BlueprintProWeb.Models.User>

@{
    ViewData["Title"] = "Architect Match";
    Layout = "~/Views/Shared/_ClientSharedLayout.cshtml";
}

<div class="container-fluid px-4 py-3">
    <!-- Title and Header -->
    <div class="d-flex justify-content-between align-items-center mb-3">
        <h2 class="fw-bold">Find Your Architect</h2>
        <a href="#" class="btn btn-outline-primary">My Matches</a>
    </div>

    <!-- Prompt Input -->
    <div class="card shadow-sm mb-4 p-3">
        <label for="clientPrompt" class="form-label fw-semibold">Describe what you are looking for:</label>
        <textarea id="clientPrompt" class="form-control" rows="3"
                  placeholder="e.g. I need a modern residential architect who designs sustainable homes..."></textarea>
        <div class="d-flex justify-content-end mt-2">
            <button class="btn btn-primary" id="searchArchitects">Search</button>
        </div>
    </div>

    <!-- Architect Profiles -->
    <div class="row" id="architectResults">
        @foreach (var architect in Model)
        {
            <div class="col-md-4 mb-4">
                <div class="card shadow-sm h-100">
                    <div class="card-body d-flex flex-column">
                        <!-- Profile Header -->
                        <div class="d-flex align-items-center mb-3">
                            <img src="~/images/@(architect.user_profilePhoto ?? "default-profile.png")"
                                 class="rounded-circle me-3 border" width="50" height="50"
                                 alt="@($"{architect.user_fname} {architect.user_lname}")" />
                            <div>
                                <h5 class="fw-bold mb-0">@($"{architect.user_fname} {architect.user_lname}")</h5>
                                <small class="text-muted">@architect.user_Specialization ?? "General Architect"</small>
                            </div>
                        </div>

                        <!-- Work Sample Placeholder -->
                        <img src="~/images/sample-work.jpg" class="img-fluid rounded mb-3"
                             alt="Work sample of @architect.user_fname" />

                        <!-- Architect Description -->
                        <p class="text-muted small flex-grow-1">
                            Specializes in @architect.user_Style ?? "various styles"
                            located in @architect.user_Location ?? "multiple locations".
                            Accepts projects with budget: @architect.user_Budget ?? "Flexible".
                        </p>

                        <!-- Rating -->
                        <div class="d-flex align-items-center mb-2">
                            <span class="text-warning">★★★★★</span>
                            <small class="ms-2">(@(architect.user_Rating?.ToString("0.0") ?? "0"))</small>
                        </div>

                        <!-- Match Button -->
                        <button class="btn btn-outline-primary w-100 mt-auto match-btn"
                                data-id="@architect.Id">
                            Match
                        </button>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

@section Scripts {
    <script>
        document.getElementById('searchArchitects').addEventListener('click', function () {
            let prompt = document.getElementById('clientPrompt').value;
            if (prompt.trim() === "") {
                alert("Please describe your desired architect.");
                return;
            }

            // Example AJAX call (replace with your real endpoint)
            fetch(/ClientInterface/SearchArchitects?query=${encodeURIComponent(prompt)})
                .then(res => res.json())
                .then(data => {
                    console.log("Architects found:", data);
                    // TODO: dynamically render architect cards here
                })
                .catch(err => console.error("Search failed:", err));
        });

        // Example Match button click
        document.querySelectorAll('.match-btn').forEach(btn => {
            btn.addEventListener('click', function () {
                let architectId = this.getAttribute('data-id');
                alert("Matched with Architect ID: " + architectId);
                // TODO: send match request to server
            });
        });
    </script>
}